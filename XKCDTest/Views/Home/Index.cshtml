@model VMComic
@{
    ViewData["Title"] = "Comic of the Day!";
}

<div class="row">
    <div class="col-sm-12">
        <div class="text-center">
            <h1>@Model?Comic?.Title</h1>
            <img alt="@Model?.Comic?.Alt" src="@Model?.Comic.Img"/>
            <h4>@Model?.Comic?.Alt</h4>
        </div>
    </div>
</div>
